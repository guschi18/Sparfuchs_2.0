{
  "name": "sparfuchs-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run data:build && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "npm run analyze",
    "data:build": "tsx scripts/build-data.ts",
    "data:update": "npm run data:build",
    "data:validate": "tsx scripts/validate-data.ts",
    "cache:warm": "tsx scripts/warm-cache.ts warm",
    "cache:test": "tsx scripts/warm-cache.ts test",
    "production:build": "npm run data:build && npm run cache:warm && next build"
  },
  "dependencies": {
    "@heroui/react": "^2.6.14",
    "@heroui/system": "^2.4.18",
    "@heroui/theme": "^2.4.18",
    "@next/bundle-analyzer": "^14.2.18",
    "critters": "^0.0.23",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "eslint-config-next": "^14.2.18",
    "framer-motion": "^12.23.12",
    "next": "^14.2.18",
    "openai": "^5.10.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/csv-parse": "^1.1.12",
    "@types/node": "^20",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
